{
  "name": "sunbird-quml-player",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build quml-library --watch=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build quml-library && node scripts/copyAssets.js",
    "postbuild-lib": "npm run build:schematics",
    "serve": "node scripts/copyAssets.js && ng serve",
    "build-web-component": "npm run build-lib && ng build quml-player-wc --output-hashing none && node ./build-wc.js",
    "test-lib": "ng test quml-library",
    "test-lib-coverage": "ng test quml-library --code-coverage=true",
    "test-lib-ci": "ng test quml-library --watch=false --code-coverage",
    "build:schematics": "npm run build --prefix projects/quml-library"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/elements": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@project-sunbird/client-services": "4.9.1",
    "@project-sunbird/sb-styles": "0.0.9",
    "@project-sunbird/telemetry-sdk": "0.0.29",
    "bootstrap": "^4.6.2",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "jquery": "^3.5.1",
    "katex": "^0.11.1",
    "lodash-es": "^4.17.21",
    "ngx-bootstrap": "^9.0.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "uuid": "^3.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular-devkit/core": "^14.2.11",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "copyfiles": "^2.4.1",
    "fs-extra": "^8.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.1",
    "tslint": "~6.1.3",
    "typescript": "~4.6.4"
  }
}
